{ 
  "manifest_version": 2,
  "name": "Getting Started Example",
  "description": "This extension showd a Google Image search result for the current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "ychartsicon.png",
    "default_title": "YCharts",
    "default_popup": "/test-angular/public/index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ]
}
